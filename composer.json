{
  "name": "wildphp/wild-irc-bot",
  "description": "A modular IRC bot written in PHP",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "WildPHP\\Core\\": "src/",
      "WildPHP\\Modules\\": "modules/"
    }
  },
  "require": {
    "php": ">=7.2.0",

    "react/event-loop": "^1.0",
    "react/socket": "^1.0",
    "evenement/evenement": "^3.0",
    "yoshi2889/collections": "^0.1",
    "yoshi2889/validation-closures": "^0.2",
    "monolog/monolog": "^2",
    "php-di/php-di": "^6.0",
    "wildphp/command-parser": "^0.1",
    "wildphp/irc-messages": "^0.2",
    "wildphp/queue": "^0.1",
    "wildphp/authenticator": "dev-master",

    "ext-pdo": "*",
    "ext-posix": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "config": {
    "platform": {
      "php": "7.2.0"
    }
  }
}
